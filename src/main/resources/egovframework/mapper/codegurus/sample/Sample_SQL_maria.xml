<mapper namespace="codegurus.sample.SampleDAO">

	<!-- 샘플 목록 조회 -->
	<select id="selectTestList" resultType="SampleVO">
		select /* selectTestList */
			*
		from test_table
	</select>

	<insert id="insertTest">
		insert /* insertTest */
		into test_table
		(
			name,
			age
		)
		values
		(
			#{name},
		 	#{age}
		)
	</insert>

</mapper>